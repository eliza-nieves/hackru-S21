{
	"manifest_version": 2,
	"name": "hackru",
	"description": "my extension",
	"version": "1.0.0",
	"icons":{
		"16": "./icon-16x16.png",
		"32": "./icon-32x32.png",
		"48": "./icon-48x48.png",
		"128": "./icon-128x128.png"
	},
	"background": {
		"scripts" : ["./background.js", "./popup.js"] 
		
	}, 
	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"<all_urls>",
		"https://www.google.com/*",
		"storage"
	]
}